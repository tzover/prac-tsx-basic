{
    "env": {
        "browser": true,
        "es2021": true
    },
    "root": true,
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": "./tsconfig.eslint.json",
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["react", "@typescript-eslint"],
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "extends": ["airbnb-typescript", "prettier"],
    "rules": {
        "import/prefer-default-export": "off",
        "react/prop-types": "off",
        "no-console": "off"
    }
}
